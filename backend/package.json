{
  "name": "owlby-backend",
  "version": "1.0.0",
  "description": "Backend server for Owlby learning platform",
  "main": "dist/serverless.js",
  "scripts": {
    "start": "node dist/serverless.js",
    "dev": "nodemon --exec ts-node src/serverless.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "ts-node src/db/migrate.ts",
    "setup:supabase": "node scripts/setup-supabase.js"
  },
  "keywords": [
    "education",
    "learning",
    "ai",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/logging": "^10.0.0",
    "@google-cloud/storage": "^7.0.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@supabase/supabase-js": "^2.49.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "expo-auth-session": "^6.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "redis": "^4.7.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setup.ts"
    ]
  }
}
